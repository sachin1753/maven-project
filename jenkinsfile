pipeline
{
	agent any
	stages
	{
		
		stage ('scm checkout')
		{
			steps
			{
				git branch: 'master', url: 'https://github.com/sachin1753/maven-project'
			}

		}
	

		stage ('compile source code')
		{
			steps
			{
				withMaven(jdk: 'javaJDK', maven: 'defaultMaven') {
				    sh 'mvn compile'
				}
			}

		}
		

	}
}
