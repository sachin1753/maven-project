pipeline
{
	agent any
	stages
	{
		
		stage ('scm checkout')
		{
			steps
			{
				git branch: 'master', url: 'https://github.com/sachin1753/maven-project'
			}

		}
	

		stage ('create EC2 Instance')
		{
			steps
			{
				withAWS(credentials: 'aws-key', region: 'us-east-1') {
				    sh 'aws ec2 run-instances --count 1 --instance-type t2.micro --key-name devops-ec2 --security-group-ids MyEC2RoleGroup'
				}
			}

		}
		

	}
}
