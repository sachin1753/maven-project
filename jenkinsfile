pipeline
{
	agent any
	stages
	{
		
		stage ('scm checkout')
		{
			steps
			{
				git branch: 'master', url: 'https://github.com/sachin1753/maven-project'
			}

		}
	

		stage ('list s3 buckets')
		{
			steps
			{
				withAWS(credentials: 'aws-key', region: 'us-east-1') {
				    sh 'aws s3 mb s3://my-bucket-sachin-feb-batch-11may2020'
				}
			}

		}
		

	}
}
